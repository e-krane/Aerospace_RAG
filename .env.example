# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=  # Optional, leave empty for local development

# Model Configuration
EMBEDDING_MODEL=Alibaba-NLP/gte-Qwen2-7B-instruct
RERANKER_MODEL=jinaai/jina-colbert-v2
LLM_MODEL=claude-3-5-sonnet-20241022
LLM_TEMPERATURE=0.1

# Model Cache Directories
HF_HOME=./models
TRANSFORMERS_CACHE=./models/transformers
TORCH_HOME=./models/torch

# Processing Configuration
BATCH_SIZE=32
MAX_WORKERS=4
CHUNK_SIZE=750
CHUNK_OVERLAP=100
EMBEDDING_DIMENSION=256  # Matryoshka reduced dimension

# Retrieval Configuration
TOP_K_INITIAL=100
TOP_K_FINAL=10
HYBRID_ALPHA=0.7  # Weight for keyword search in hybrid retrieval

# Monitoring and Logging
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com
LOG_LEVEL=INFO
LOG_FILE=logs/aerospace_rag.log

# Redis Configuration (Optional)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
CACHE_TTL_HOURS=24

# Development Settings
DEBUG=False
ENABLE_PROFILING=False
